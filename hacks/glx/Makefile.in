	      $(UTILS_BIN)/visual.o $(UTILS_BIN)/resources.o
xscreensaver-gl-helper: $(HELPER_OBJS)
	$(CC) $(LDFLAGS) -o $@ $(HELPER_OBJS) $(LIBS) $(X_LIBS) $(HACK_POST2)


