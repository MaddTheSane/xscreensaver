rm -f conftest*
LIBS="$ac_save_LIBS"

fi
if eval "test \"`echo '$ac_cv_lib_'$ac_lib_var`\" = yes"; then
  echo "$ac_t""yes" 1>&6
  X_LIBS="$X_LIBS -lgen"
else
  echo "$ac_t""no" 1>&6
fi

      fi
    ;;
  esac
echo $ac_n "checking for XPointer""... $ac_c" 1>&6
echo "configure:3512: checking for XPointer" >&5
