  if test "$enable_locking" = yes; then
    echo "$as_me:$LINENO: result: locking disabled: it doesn't work on MacOS X" >&5
echo "${ECHO_T}locking disabled: it doesn't work on MacOS X" >&6
    enable_locking=no
    cat >>confdefs.h <<\_ACEOF
