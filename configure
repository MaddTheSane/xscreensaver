  ""|no) : ;;
  /*)
    cat >>confdefs.h <<_ACEOF
#define PASSWD_HELPER_PROGRAM "$with_passwd_helper"
_ACEOF

    have_passwd_helper=yes;;
  *)
    echo "error: --with-passwd-helper needs full pathname of helper (not '$with_passwd_helper')." >&2
    exit 1
esac


