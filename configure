   case "$with_shadow" in
    yes) ;;
    no)  ;;

    /*)
     echo "$as_me:$LINENO: checking for shadow password headers" >&5
echo $ECHO_N "checking for shadow password headers... $ECHO_C" >&6
     d=$with_shadow/include
     if test -d $d; then
       X_CFLAGS="-I$d $X_CFLAGS"
