#include "../config.h"

#ifdef HAVE_READ_DISPLAY_EXTENSION
# define READIMAGE_DEF -DHAVE_READ_DISPLAY_EXTENSION
#else
# define READIMAGE_DEF
#endif

#ifdef NO_SELECT
# define NOSELECT_DEF -DNO_SELECT
#else
# define NOSELECT_DEF
#endif

    DEFINES = R5ISMS NOSELECT_DEF READIMAGE_DEF
       SRCS = colors.c fade.c grabscreen.c hsv.c resources.c spline.c \
	      usleep.c visual.c xroger.c yarandom.c
       OBJS = colors.o fade.o grabscreen.o hsv.o resources.o spline.o \
	      usleep.o visual.o xroger.o yarandom.o
   TARFILES = README Imakefile ad2c $(SRCS) colors.h fade.h grabscreen.h \
	      hsv.h resources.h spline.h usleep.h visual.h yarandom.h \
	      version.h

all:: $(OBJS)

DependTarget()

echo_tarfiles:
	@echo $(TARFILES)
